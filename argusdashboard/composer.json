{
    "name": "novel-t/argus_dashboard",
    "license": "MIT",
    "type": "project",
    "description": "ARGUS Application",
    "autoload": {
        "psr-0": { "": "src/", "SymfonyStandard": "app/" }
    },
    "require": {
        "php"                                  : ">=7.2.0",
        "ext-pdo_sqlite"                       : "*",
        "doctrine/doctrine-bundle"             : "~1.5",
        "doctrine/doctrine-fixtures-bundle"    : "~2.2",
        "doctrine/orm"                         : "~2.5",
        "erusev/parsedown"                     : "~1.5",
        "incenteev/composer-parameter-handler" : "~2.1",
        "ircmaxell/password-compat"            : "~1.0",
        "leafo/scssphp"                        : "~0.1.5",
        "patchwork/jsqueeze"                   : "~1.0",
        "sensio/distribution-bundle"           : "~3.0.28",
        "sensio/framework-extra-bundle"        : "~3.0",
        "symfony/assetic-bundle"               : "~2.6",
        "symfony/monolog-bundle"               : "~3.0",
        "symfony/swiftmailer-bundle"           : "~2.3",
        "symfony/symfony"                      : "~2.7",
        "jms/serializer-bundle"                : "~1.0",
        "friendsofsymfony/rest-bundle"         : "~1.7",
        "friendsofsymfony/user-bundle"         : "~2.0",
        "knplabs/knp-paginator-bundle"         : "~2.5",
        "pear/http_request2"                   : "~2.3",
        "stof/doctrine-extensions-bundle"      : "~1.2",
        "jms/i18n-routing-bundle"              : "~2.0.4",
        "jms/translation-bundle"               : "~1.1",
        "st/flagiconcss-bundle"                : "~1.0",
        "tetranz/select2entity-bundle"         : "~2.0",
        "friendsofsymfony/jsrouting-bundle"    : "~1.6",
        "lexik/jwt-authentication-bundle"      : "~2.1",
        "nelmio/cors-bundle"                   : "~1.5",
        "mk-j/php_xlsxwriter"                  : "~0.32",
        "gesdinet/jwt-refresh-token-bundle"    : "~0.2",
        "symfony/filesystem"                   : "~2.8",
        "twig/extensions"                      : "^1.5"
    },
    "require-dev": {
        "sensio/generator-bundle": "~2.3",
        "phpunit/phpunit": "^5.6"
    },
    "scripts": {
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "secure-http":false
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        }
    }
}
